{
  "expo": {
    "name": "SearchBotApp",
    "slug": "searchbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.searchbot.app",
      "supportsTablet": true,
      "buildNumber": "1"
    },
    "android": {
      "package": "com.searchbot.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "./plugins/withAndroidSupportExclusions"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1864d6b1-b270-45a6-8ed7-b23c367c4d2c"
      }
    },
    "owner": "niktolik88"
  }
}
